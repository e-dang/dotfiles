{
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": true,
    "C_Cpp.updateChannel": "Default",
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "clang-format.executable": "/usr/local/bin/clang-format",
    "cmake.configureOnOpen": false,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "dotfiles.repository": "https://github.com/e-dang/dotfiles.git",
    "dotfiles.targetPath": "~/dotfiles",
    "dotfiles.installCommand": "bash $(~/dotfiles/install-profile -f docker-linux)",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.fontFamily": "MesloLGS NF",
    "editor.highlightActiveIndentGuide": true,
    "editor.insertSpaces": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.minimap.maxColumn": 80,
    "editor.quickSuggestionsDelay": 5,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        120
    ],
    "editor.tabSize": 4,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "jinja-html": "html"
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespace": true,
    "git.detectSubmodulesLimit": 9999,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "jupyter.askForKernelRestart": false,
    "prettier.disableLanguages": ["html"],
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.mypyEnabled": false,
    "python.testing.pytestEnabled": true,
    "remote.containers.defaultExtensions": [
        "mhutchie.git-graph",
        "mkxml.vscode-filesize",
        "redhat.vscode-yaml"
    ],
    "search.useGlobalIgnoreFiles": true,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.rendererType": "auto",
    "terminal.integrated.fontSize": 14,
    "window.zoomLevel": 0,
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 20,
    "workbench.editor.wrapTabs": true,
    "workbench.colorTheme": "Custom Atom One Dark",
    "workbench.iconTheme": "file-icons",
    "yaml.format.printWidth": 120,
    "yaml.format.proseWrap": "never",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "files.associations": {
        "*.ejs": "jinja-html"
    },
    "arduino.additionalUrls": [
        "http://arduino.esp8266.com/stable/package_esp8266com_index.json"
    ]
}