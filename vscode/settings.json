{
  "autoDocstring.docstringFormat": "google",
  "autoDocstring.startOnNewLine": true,
  "C_Cpp.updateChannel": "Default",
  "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "clang-format.executable": "/usr/local/bin/clang-format",
  //   "cmake.configureOnOpen": false,
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  //   "[django-html]": {
  //     "editor.defaultFormatter": "batisteo.vscode-django"
  //   },
  "dotfiles.repository": "https://github.com/e-dang/dotfiles.git",
  "dotfiles.targetPath": "~/dotfiles",
  "dotfiles.installCommand": "bash $(~/dotfiles/install-profile docker)",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.fontFamily": "MesloLGS NF",
  "editor.insertSpaces": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.maxColumn": 80,
  "editor.quickSuggestionsDelay": 5,
  "editor.renderWhitespace": "none",
  "editor.rulers": [120],
  "editor.suggest.insertMode": "insert",
  "editor.tabSize": 4,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "jinja-html": "html",
    "django-html": "html"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "**/*.html": "html",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "charts/*.yaml": "helm"
  },
  "git.detectSubmodulesLimit": 9999,
  "[helm]": {
    "editor.tabSize": 2
  },
  "html.format.templating": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.askForKernelRestart": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.mypyEnabled": false,
  "python.testing.pytestEnabled": false,
  "redhat.telemetry.enabled": false,
  "remote.containers.defaultExtensions": [
    "mhutchie.git-graph",
    "mkxml.vscode-filesize",
    "editorconfig.editorconfig",
    "eamodio.gitlens",
    "yzhang.markdown-all-in-one",
    "alexkrechik.cucumberautocomplete"
  ],
  "search.useGlobalIgnoreFiles": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    }
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "MesloLGS, MesloLGS NF",
  "terminal.integrated.fontSize": 14,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true
  },
  "typescript.tsserver.log": "off",
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path.mac": "/Users/ericdang/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
},
  "window.zoomLevel": 0,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 20,
  "workbench.editor.wrapTabs": true,
  "workbench.colorTheme": "Custom Atom One Dark",
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.revealIfOpen": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[yaml]": {
    "editor.tabSize": 2
  }
}
