{
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.startOnNewLine": true,
    "C_Cpp.updateChannel": "Default",
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "clang-format.executable": "/usr/local/bin/clang-format",
    "cmake.configureOnOpen": false,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[django-html]": {
        "editor.defaultFormatter": "batisteo.vscode-django"
    },
    "dotfiles.repository": "https://github.com/e-dang/dotfiles.git",
    "dotfiles.targetPath": "~/dotfiles",
    "dotfiles.installCommand": "bash $(~/dotfiles/install-profile docker-dev)",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.detectIndentation": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.fontFamily": "MesloLGS NF",
    "editor.highlightActiveIndentGuide": true,
    "editor.insertSpaces": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.minimap.maxColumn": 80,
    "editor.quickSuggestionsDelay": 5,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "none",
    "editor.rulers": [120],
    "editor.suggest.insertMode": "insert",
    "editor.tabSize": 4,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "jinja-html": "html",
        "django-html": "html"
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "**/*.html": "html",
        "**/templates/**/*.html": "django-html",
        "**/templates/**/*": "django-txt",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements"
    },
    "git.detectSubmodulesLimit": 9999,
    "html.format.templating": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "jupyter.askForKernelRestart": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.mypyEnabled": false,
    "python.testing.pytestEnabled": false,
    "remote.containers.defaultExtensions": [
        "mhutchie.git-graph",
        "mkxml.vscode-filesize",
        "editorconfig.editorconfig",
        "eamodio.gitlens",
        "yzhang.markdown-all-in-one",
        "alexkrechik.cucumberautocomplete"
    ],
    "search.useGlobalIgnoreFiles": true,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.fontFamily": "MesloLGS, MesloLGS NF",
    "terminal.integrated.rendererType": "canvas",
    "terminal.integrated.fontSize": 14,
    "window.zoomLevel": 0,
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 20,
    "workbench.editor.wrapTabs": true,
    "workbench.colorTheme": "Custom Atom One Dark",
    "workbench.iconTheme": "file-icons",
    "workbench.editor.revealIfOpen": true,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ]
}
